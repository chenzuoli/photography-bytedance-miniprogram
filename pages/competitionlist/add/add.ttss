/* pages/competitionlist/add/add.ttss */
.page {
  height: 100vh;
  width: 100vw;
}
.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}

.btnSubmit {
  margin: 40px;
  margin-left: 100px;
  margin-right: 100px;
  width: 130px;
  font-size: 30rpx;
}


.page.show {
  overflow: hidden;
}

.switch-sex::after {
  content: "\e716";
}

.switch-sex::before {
  content: "\e7a9";
}

.switch-music::after {
  content: "\e66a";
}

.switch-music::before {
  content: "\e6db";
}

.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}

.btnSubmit {
  margin: 40px;
  margin-left: 100px;
  margin-right: 100px;
  width: 130px;
  font-size: 30rpx;
}

.container {
  display: flex;
  justify-content: flex-start;
  width: 100%;
  /* height: 127vh; */
}

.comment1-description {
  padding: 30rpx 0;
  display: flex;
  flex-direction: row;
  margin-top: -50px;
}

.stars {
  background: url(data:image/png;base64,);
  width: 50rpx;
  height: 50rpx;
  background-size: 50rpx 50rpx;
  margin-left: 30rpx;
  transition: 600ms;
}

.on {
  background: url(data:image/png;base64,);
  width: 50rpx;
  height: 50rpx;
  background-size: 50rpx 50rpx;
  transition: 1s;
  transform: scale(1, 1);
}

.weui-cells_after-title {
  width: 92%;
  margin: 0 auto;
  /* border: 1px solid #ddd; */
  border: 1px solid #b9dd08;
}

.weui-textarea {
  padding-left: 10px;
  padding-top: 10px;
}

.weui-textarea-counter {
  padding-left: 10px;
  padding-bottom: 10px;
}

.action .action-photo {
  margin-top: 30rpx;
  padding: 40rpx 0 40rpx 50rpx;
  background-color: inherit;
  border-radius: 8rpx;
}

.action .action-photo image {
  width: 120rpx;
  height: 90rpx;
  margin-left: 25rpx;
  position: relative;
  overflow: visible;
  vertical-align: top;
}

.action .action-photo image icon {
  position: absolute;
  top: -20rpx;
  right: -20rpx;
}

.action .action-photo .add {
  display: inline-block;
  height: 90rpx;
  width: 120rpx;
  margin-left: 30rpx;
  margin-top: -15rpx;
  font-size: 24rpx;
  border: 2rpx dotted #ccc;
  border-radius: 8rpx;
  color: #ccc;
  text-align: center;
  line-height: 90rpx;
}

.action .action-input {
  margin-top: 30rpx;
  /* padding-left: 60rpx; */
  padding: center;
  /* background-color: #fff; */
  background-color: inherit;
}

.action .action-input input {
  padding-top: 40rpx;
  padding-bottom: -100rpx;
  width: 100%;
  color: rgba(10, 10, 10, 0.76);
}

.action .action-input input:first-child {
  border-bottom: 1px solid #ccc;
}

.action .action-input input:last-child {
  border-bottom: 1px solid #ccc;
}

.action-input .placeholder-input {
  color: rgba(10, 10, 10, 0.308);
}

.action .action-submit button {
  /* margin: 40rpx 80rpx 40rpx 40rpx; */
  margin-top: 50px;
  width: 100%;
  background-color: #f2f2f2;
  padding: center;
}
